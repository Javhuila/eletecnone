    }                                         

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Boton de Igual 
        jTextField1.setText(""+CALC.calcular_result(jTextField1.getText()));
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Vaciar el texto por completo
        jTextField1.setText(CALC.limpiar(""));
    }                                        

    private void jButton17ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // Boton Sin == SENO
        CALC.Seno(jTextField1.getText());
        jTextField1.setText(""+CALC.calcular_result(jTextField1.getText()));
    }                                         

    private void jButton18ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // Boton Cos == COSENO
        CALC.Coseno(jTextField1.getText());
        jTextField1.setText(""+CALC.calcular_result(jTextField1.getText()));
    }                                         

    private void jButton19ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // Boton Tan == TANGENTE
        CALC.Tangente(jTextField1.getText());
        jTextField1.setText(""+CALC.calcular_result(jTextField1.getText()));
    }                                         

    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // Caja de Texto
        
    }                                           

