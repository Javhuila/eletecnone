    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Adiccionar == Sumar
        CALC.Sumar(jTextField1.getText());
        jTextField1.setText("");
    }                                        

    private void jButton16ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // Sustraer == Restar
        CALC.Restar(jTextField1.getText());
        jTextField1.setText("");
    }                                         

